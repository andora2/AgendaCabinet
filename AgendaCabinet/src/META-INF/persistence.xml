<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="AgendaCabinet" transaction-type="JTA">
		<jta-data-source>jdbc/AgendaCabinet</jta-data-source>
		<class>model.Customer</class>
		<class>model.Offer</class>
		<class>model.Termin</class>
		<class>model.User</class>
		<class>model.WorkType</class>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/AgendaCabinet"/>
			<property name="javax.persistence.jdbc.user" value="WASUSER"/>
			<property name="javax.persistence.jdbc.password" value="wasuser"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
